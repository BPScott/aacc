{
  "name": "@aacc/rollup-babel-js",
  "version": "0.0.0",
  "description": "Example hybrid ESM/CJS JavaScript build with Rollup and Babel",
  "author": "Aaron Casanova <aaronccasanova@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "@aacc:registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "browser": "dist/browser/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "dev": "npm-run-all --parallel 'build:* -- --watch'",
    "build": "npm-run-all --parallel build:*",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "lint": "TIMING=1 eslint src --ext .js,.jsx",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "the-answer": "^1.0.0"
  },
  "devDependencies": {
    "@aacc/eslint-config": "^0.0.0",
    "@aacc/tsconfigs": "^0.0.0",
    "@aacc/browserslist-config": "^0.0.0",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "rollup": "^2.70.2",
    "typescript": "^4.6.3"
  },
  "browserslist": [
    "extends @aacc/browserslist-config"
  ]
}
