{
  "name": "@aacc/rollup-babel-ts",
  "version": "0.0.0",
  "description": "Example hybrid ESM/CJS JavaScript build with Rollup and Babel",
  "author": "Aaron Casanova <aaronccasanova@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "@aacc:registry": "https://registry.npmjs.org"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/browser/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "the-answer": "^1.0.0"
  },
  "devDependencies": {
    "@aacc/tsconfigs": "^0.0.0",
    "@aacc/browserslist": "^0.0.0",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "rollup": "^2.70.2",
    "typescript": "^4.6.3"
  },
  "browserslist": [
    "extends @aacc/browserslist"
  ]
}
