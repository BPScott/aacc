{
  "name": "@aacc/webpack-babel-ts",
  "version": "0.0.0",
  "description": "Example hybrid ESM/CJS JavaScript build with Webpack and Babel",
  "author": "Aaron Casanova <aaronccasanova@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "@aacc:registry": "https://registry.npmjs.org"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "browser": "dist/browser/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "dev": "npm-run-all --parallel 'build:* -- --watch'",
    "build": "npm-run-all --parallel build:*",
    "build:js": "webpack --mode=production",
    "build:types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "lint": "TIMING=1 eslint src --ext .js,.jsx,.ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "the-answer": "^1.0.0"
  },
  "devDependencies": {
    "@aacc/babel-preset": "^0.0.0",
    "@aacc/browserslist-config": "^0.0.0",
    "@aacc/eslint-config": "^0.0.0",
    "@aacc/tsconfigs": "^0.0.0",
    "babel-loader": "^8.2.5",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": [
    "extends @aacc/browserslist-config"
  ]
}
